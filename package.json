{
  "name": "skybooker-monorepo",
  "private": true,
  "workspaces": [
    "airline-booking-system/backend",
    "airline-booking-system/frontend"
  ],
  "scripts": {
    "start": "yarn workspace airline-booking-backend run build && yarn workspace airline-booking-backend run start",
    "dev": "yarn workspace airline-booking-backend run dev",
